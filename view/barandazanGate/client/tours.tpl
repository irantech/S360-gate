{load_presentation_object filename="reservationTour" assign="objReservationTour"}{assign var="originCountryData" value='all'}{if $smarty.const.SEARCH_ORIGIN_COUNTRY neq 'all'}    {assign var="originCountryData" value=$objReservationTour->FindCountry($smarty.const.SEARCH_ORIGIN_COUNTRY)}    {$originCountryData['comments_visa']=''}{/if}{assign var="originCityData" value='all'}{if $smarty.const.SEARCH_ORIGIN_CITY neq 'all'}    {assign var="originCityData" value=$objReservationTour->FindCity($smarty.const.SEARCH_ORIGIN_CITY)}{/if}{assign var="originData" value=['country'=>$originCountryData,'city'=>$originCityData]}{assign var="jsonOriginData" value=str_replace('"', "'",$originData|json_encode:256:64)}{assign var="destinationCountryData" value='all'}{if $smarty.const.SEARCH_DESTINATION_COUNTRY neq 'all'}    {assign var="destinationCountryData" value=$objReservationTour->FindCountry($smarty.const.SEARCH_DESTINATION_COUNTRY)}    {$destinationCountryData['comments_visa']=''}{/if}{assign var="destinationCityData" value='all'}{if $smarty.const.SEARCH_DESTINATION_CITY neq 'all'}    {assign var="destinationCityData" value=$objReservationTour->FindCity($smarty.const.SEARCH_DESTINATION_CITY)}{/if}{assign var="jsonDestinationData" value=str_replace(["\n", "\r", " "], "",$destinationData)}{assign var="destinationData" value=[    'country'=>$destinationCountryData,    'city'=>$destinationCityData]}{assign var="jsonDestinationData" value=str_replace(["\n", "\r", " "], "",$destinationData)}{assign var="jsonDestinationData" value=str_replace('"', "'",$jsonDestinationData|json_encode:256:64)}<link href="/gds/dist/css/tourApp.css" rel='stylesheet' /><div class="w-100 position-unset" id="vueApp">    <tour-app :const-data="{            'date':`{$smarty.const.MILADI_SEARCH_DATE}`,            'type':`{$smarty.const.SEARCH_TOUR_TYPE}`,            'lang':`{$smarty.const.SOFTWARE_LANG}`,            'special':`{$smarty.const.SEARCH_TOUR_SPECIAL}`,            'client_id':`{$smarty.const.CLIENT_ID}`,            'origin_country':`{$smarty.const.SEARCH_ORIGIN_COUNTRY}`,            'origin_city':`{$smarty.const.SEARCH_ORIGIN_CITY}`,            'destination_country':`{$smarty.const.SEARCH_DESTINATION_COUNTRY}`,            'destination_city':`{$smarty.const.SEARCH_DESTINATION_CITY}`,            'destination_data':`{$jsonDestinationData}`,            'origin_data':`{$jsonOriginData}`,            }"></tour-app></div>    {assign var="moduleData" value=[    'service'=>'Tour',    'origin'=>$smarty.const.SEARCH_ORIGIN_CITY,    'destination'=>$smarty.const.SEARCH_DESTINATION_CITY    ]}    {include file="`$smarty.const.FRONT_CURRENT_CLIENT`textSearchResults.tpl"    moduleData=$moduleData}    {include file="`$smarty.const.FRONT_CURRENT_CLIENT`faqs.tpl"    moduleData=$moduleData}    {include file="`$smarty.const.FRONT_CURRENT_CLIENT`articles.tpl"    moduleData=$moduleData}<script src="/gds/dist/js/tourApp.js"></script>