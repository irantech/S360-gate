    {assign var='cityName' value='##Chaboksar##'}
    {assign var='title' value='##Ahuan4##'}

{/if}

<div class="main zarvan">
    <div class="top-wrapper">

	<div class="row header-zarvan">
		<img src="project_files/images/header-zarvan.jpg" class="zarvan-head" title="##Zarvan##" alt="زروان##Zarvan##" border="0" width="100%">
	</div>
		<div class="head-info">
        <div class="logo zarvan-logo">
            <img src="project_files/images/logo-zarvan.jpg" title="##Zarvan##" alt="##Zarvan##" border="0" width="120px">
        </div>

        <div class="title-top">
            <span>##Approvereservation## : ##Zarvan##</span>
			<span>
                <b>##Numberreservation##: </b>
                <i>{$list[0]['factor_number']}</i>
            </span>
        </div>

        <div class="voucher-number">
            <span>
                <b>##Issuedate##: </b>
                <i style="display: inline-block;">{$objFunctions->set_date_payment($list[0]['payment_date'])}</i>
            </span>
        </div>
    </div>


	<div class="row">

		<div class="info-voucher-head">
			<span class="col-xs-4"> ##Namehotel##: {$list[0]['hotel_name']} </span>
			<span class="col-xs-8"> ##City##: {$list[0]['city_name']} </span>
			<span class="col-xs-4"> ##Enterdate##: <i>{$list[0]['start_date']} </i></span>
			<span class="col-xs-4"> ##Exitdate##: <i> {$list[0]['end_date']} </i></span>
			<span class="col-xs-4"> ##Stayigtime##: {$list[0]['number_night']}  ##Timenight## </span>
			<span class="col-xs-12"> ##Addresshotel##: {$list[0]['hotel_address']} </span>
		</div>
		</div>

		<div class="info-voucher-main">
			<span class="col-xs-4 border-b border-l fontB text-center">##Guestname##</span>
			<span class="col-xs-4 border-b border-l fontB text-center">##Typeroom##</span>
			<span class="col-xs-4 border-b fontB text-center">##PossibilitiesRoom##</span>
			{foreach key=key item=item from=$list}

                {assign var='roomFacilities' value=$objResult->getRoomFacilities($item['hotel_room_prices_id'])}
				<span class="col-xs-4 border-b border-l">
					{if $item['passenger_name'] neq ''}
						{$item['passenger_name']} {$item['passenger_family']}
                    {elseif $item['passenger_name_en'] neq ''}
                        {$item['passenger_name_en']} {$item['passenger_family_en']}
					{else}
						----------
					{/if}
				</span>
				<span class="col-xs-4 border-b border-l">{$item['room_name']}</span>
				<span class="col-xs-4 border-b">
					{if $roomFacilities['breakfast'] eq 'yes'}##Breakfast## - {/if}
					{if $roomFacilities['lunch'] eq 'yes'}##Lunch## - {/if}
					{if $roomFacilities['dinner'] eq 'yes'}##Dinner##{/if}
				</span>
			{/foreach}
		</div>

		<div class="info-voucher-footer">
			<div class="row">
				<div class="col-xs-12">
				##Bookingrulesguest## :
				</div>
				<div class="col-xs-12">
